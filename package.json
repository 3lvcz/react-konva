{
  "name": "react-kinetic",
  "version": "0.2.0",
  "author": "Mikhail <freiksenet@gmail.com> Novikov",
  "description": "Using KineticJS library with React",
  "keywords": [
    "canvas",
    "kinetic",
    "react",
    "react-component",
    "browser"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/freiksenet/react-kinetic.git"
  },
  "peerDependencies": {
    "kinetic": "5.0.6",
    "react": "^0.11.0"
  },
  "devDependencies": {
    "kinetic": "5.0.6",
    "react": "^0.11.0",
    "jshint": "^2.5.6",
    "jsx-loader": "^0.11.2",
    "karma-chrome-launcher": "^0.1.5",
    "karma-cli": "0.0.4",
    "karma-firefox-launcher": "^0.1.3",
    "karma-jasmine": "^0.1.5",
    "karma-webpack": "^1.2.2",
    "webpack": "^1.4.4",
    "webpack-dev-server": "^1.6.5"
  },
  "main": "./react-kinetic",
  "scripts": {
    "clean": "rm -r build",
    "umd": "webpack",
    "demo": "npm run umd && cp -r demo build/ && cp index.html build/",
    "umd-min": "webpack --config webpack.config.min.js",
    "start": "webpack-dev-server -d --colors --progress",
    "test": "jshint ./react-kinetic.js ./src/*.js && karma start --single-run --browsers Firefox",
    "test-watch": "karma start",
    "gh-pages": "./scripts/gh-pages.sh"
  }
