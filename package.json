{
  "name": "react-konva",
  "version": "0.3.0",
  "author": "Olim <olim.mail@gmail.com> Saidov, Mikhail <freiksenet@gmail.com> Novikov",
  "description": "Using Konva library with React",
  "keywords": [
    "canvas",
    "konva",
    "react",
    "react-component",
    "browser"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/olimsaidov/react-konva.git"
  },
  "peerDependencies": {
    "react": "^0.12.0"
  },
  "devDependencies": {
    "konva": "git+https://github.com/olimsaidov/konva.git",
    "react": "^0.12.0",
    "jshint": "^2.5.6",
    "jsx-loader": "^0.12.0",
    "karma-chrome-launcher": "^0.1.5",
    "karma-cli": "0.0.4",
    "karma-firefox-launcher": "^0.1.3",
    "karma-jasmine": "^0.1.5",
    "karma-webpack": "^1.2.2",
    "webpack": "^1.4.4",
    "webpack-dev-server": "^1.6.5"
  },
  "main": "./react-konva",
  "scripts": {
    "clean": "rm -r build",
    "umd": "webpack",
    "demo": "npm run umd && cp -r demo build/ && cp index.html build/",
    "umd-min": "webpack --config webpack.config.min.js",
    "start": "webpack-dev-server -d --colors --progress",
    "test": "jshint ./react-konva.js ./src/*.js ./test/*.js && karma start --single-run --browsers Firefox",
    "test-watch": "karma start",
    "gh-pages": "./scripts/gh-pages.sh"
